<bindings wsdlLocation="retailer.wsdl" xmlns="https://jakarta.ee/xml/ns/jaxws" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jaxb="https://jakarta.ee/xml/ns/jaxb">
    <bindings node="wsdl:definitions">
        <package name="pricequote.retailer.client"/>
    </bindings>
    <bindings node="wsdl:definitions/wsdl:types/xsd:schema[@targetNamespace='http://example.org/retailer']"  >
        <jaxb:schemaBindings >
            <jaxb:package name="pricequote.retailer.client"/>
        </jaxb:schemaBindings>
    </bindings>
</bindings>
